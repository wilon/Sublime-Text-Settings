jumpbox_ip.ip_id', '=', 'jumpbox_ipgroup_ip.ip_id')\n            ->where('ipgroup_id', $ipgroupId)\n            ->get();\n        if ($self) {\n            return $ipRes;\n        }\n        $this->msg($ipRes);\n    }\n\n    /**\n     * 用户所有IP列表\n     */\n    public function actionList()\n    {\n        // 判断参数\n        $uid = $_GET['uid'];\n        if (!$uid) {\n            $this->msg([], 101, 'need uid');\n        }\n        // 获取所有数据\n        $ipgroupRes = $this->actionGetIpgroup(true);\n        foreach ($ipgroupRes as $ipgroup) {\n            $ipgroupIdArr[] = $ipgroup->ipgroup_id;\n        }\n        $ipRes = DB::table('jumpbox_ipgroup_ip')\n            ->rightJoin('jumpbox_ip', 'jumpbox_ip.ip_id', '=', 'jumpbox_ipgroup_ip.ip_id')\n            ->whereIn('ipgroup_id', $ipgroupIdArr)\n            ->get();\n        $this->msg($ipRes);\n    }\n\n    /**\n     * 搜索\n     */\n    public function actionSearch()\n    {\n        // 判断参数\n        $uid = $_GET['uid'];\n        if (!$uid) {\n            $this->msg([], 101, 'need uid');\n        }\n        $keyword = trim($_GET['keyword']);\n        if (!$keyword) {\n            $this->msg([], 101, 'need keyword');\n        }\n        $keyword = \"/$keyword/i\";\n        // 获取所有数据\n        $ipgroupRes = $this->actionGetIpgroup(true);\n        foreach ($ipgroupRes as $ipgroup) {\n            $ipgroupIdArr[] = $ipgroup->ipgroup_id;\n        }\n        $ipRes = DB::table('jumpbox_ipgroup_ip')\n            ->rightJoin('jumpbox_ip', 'jumpbox_ip.ip_id', '=', 'jumpbox_ipgroup_ip.ip_id')\n            ->whereIn('ipgroup_id', $ipgroupIdArr)\n            ->get();\n        foreach ($ipRes as $ip) {\n            $ipResBetter[$ip->ipgroup_id][] = $ip;\n        }\n        // 筛选结果\n        $searchRes = [];\n        foreach ($ipgroupRes as $ipgroup) {\n            $ipgroup->ip_list = $ipResBetter[$ipgroup->ipgroup_id] ?: [];\n            if (preg_match($keyword, $ipgroup->ipgroup_name)) {\n                $searchRes[] = $ipgroup;\n         